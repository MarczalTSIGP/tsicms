
<div class="card card-profile">
  <div class="card-header" style="background-image: url(https://images.unsplash.com/photo-1501446690852-da55df7bfe07?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=7efe70d2c15585f98ff8e759fc8b6ea3&w=1000&q=80);"></div>
  <div class="card-body text-center">
    <img class="card-profile-img" src="data:image/png;base64,">
    <h3 class="mb-3"><%= @professor.name %></h3>
    <p class="mb-4"><%= @professor.professor_title.description %> - <%= @professor.professor_title.abbrev %></p>
    <p class="mb-4"> <%= @professor.occupation_area %></p>
    <p class="mb-4">Email: <%= @professor.email %></p>
    <p class="mb-4"><%= @professor.professor_category.name %></p>
  </div>
</div>

<div class="d-flex">
  <%= link_to "Voltar", admins_professors_path, class: 'btn btn-secondary' %>
</div>
